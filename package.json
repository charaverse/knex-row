{
  "name": "@charaverse/knex-row",
  "version": "0.0.0-nightly-20211215-155441",
  "description": "Lightweight row interface for Knex.js",
  "keywords": [
    "knex",
    "database"
  ],
  "license": "MIT",
  "author": "Ted Kesgar <t.kesgar@outlook.com> (https://tkesgar.space)",
  "contributors": [
    "Ted Kesgar <t.kesgar@outlook.com> (https://tkesgar.space)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charaverse/knex-row.git"
  },
  "bugs": {
    "url": "https://github.com/charaverse/knex-row/issues"
  },
  "homepage": "https://charaverse.github.io/knex-row/",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist/**"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "watch": "tsc -w",
    "lint": "eslint . --ext .js,.ts && tsc --noEmit",
    "format": "prettier --write '**/*'",
    "test": "jest",
    "clean": "rm -rf dist/**",
    "build": "tsc",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "docs": "typedoc",
    "prepack": "npm run clean && npm run build",
    "docker:up": "docker-compose --project-directory docker up -d",
    "docker:down": "docker-compose --project-directory docker down"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@tkesgar/reno": "^1.0.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "knex": "^0.95.1",
    "lint-staged": "^12.1.2",
    "mysql2": "^2.2.5",
    "prettier": "~2.5.0",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.22.10",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "knex": ">=0.95"
  }
}
